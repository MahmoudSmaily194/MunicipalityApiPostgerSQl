{
  "services": {
    "web": {
      "build": {
        "command": "dotnet restore SawirahMunicipalityWeb.csproj && dotnet build SawirahMunicipalityWeb.csproj -c Release"
      },
      "start": {
        "command": "dotnet run --project SawirahMunicipalityWeb.csproj --urls=http://0.0.0.0:$PORT"
      },
      "env": {
        "DOTNET_CLI_TELEMETRY_OPTOUT": "1",
        "ASPNETCORE_URLS": "http://0.0.0.0:$PORT"
      }
    }
  }
}
